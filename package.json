{
  "name": "component-dom",
  "version": "1.0.8",
  "description": "DOM traversal, manipulation and events aggregate library",
  "keywords": [
    "html",
    "dom",
    "ui",
    "jquery"
  ],
  "license": "MIT",
  "browser": {
    "classes": "component-classes",
    "css": "component-css",
    "delegate": "component-delegate",
    "each": "component-each",
    "event": "component-event",
    "keys": "object-keys",
    "query": "component-query",
    "text": "text-content",
    "traverse": "yields-traverse",
    "value": "component-value"
  },
  "testling": {
    "harness": "mocha",
    "scripts": [
      "dom.js",
      "test/*.js"
    ],
    "browsers": [
      "chrome/22"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/component/dom.git"
  },
  "dependencies": {
    "component-classes": "1.2.3",
    "component-css": "0.0.6",
    "component-delegate": "0.2.3",
    "component-each": "0.2.6",
    "component-event": "0.1.4",
    "component-query": "0.0.3",
    "component-value": "1.1.0",
    "domify": "1.3.2",
    "matches-selector": "1.2.0",
    "object-keys": "1.0.3",
    "text-content": "0.0.3",
    "to-function": "2.0.6",
    "trim": "0.0.1",
    "yields-traverse": "0.1.1"
  }
}
